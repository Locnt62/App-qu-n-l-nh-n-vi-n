<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.o7.planning.qlnhanvien">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Quản lý nhân viên"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuChinh"
            android:label="Trang chủ"
    ></activity>
        <activity android:name=".PhongBanActivity"
            android:label="Phòng ban"
            ></activity>
        <activity android:name=".NhanVienPhongBanActivity"
            android:label="Danh sách nhân viên"
            ></activity>
        <activity android:name=".NhanVienActivity"
            android:label="Danh sách nhân viên"
            ></activity>
        <activity android:name=".ThemNhanVienActivity"
            android:label="Thêm nhân viên"
            android:parentActivityName=".NhanVienActivity"
            ></activity>
        <activity android:name=".CapNhatNhanVien"
            android:label="Sửa nhân viên"
            ></activity>
        <activity android:name=".ChiTietNhanVienActivity"
            android:label="Chi tiết nhân viên"
            ></activity>
        <activity android:name=".ThietLapMatKhau"
            android:label="Đổi mật khẩu"
            ></activity>

    </application>

</manifest>